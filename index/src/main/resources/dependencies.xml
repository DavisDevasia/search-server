<?xml version="1.0" encoding="UTF-8"?>
<table_dependencies>

	<!-- Artist  -->
	<group root_table='artist'>
		<table name='artist'>
			<join src_field='id' target_field='artist'>
				<table name='artist_alias' />
			</join>
			<join src_field='id' target_field='artist'>
				<table name='artist_tag' />
			</join>
			<join src_field='id' target_field='artist'>
				<table name='artist_credit_name' />
			</join>
		</table>
	</group>
	
	<!-- Label  -->
	<group root_table='label'>
		<table name='label'>
			<join src_field='id' target_field='label'>
				<table name='label_alias' />
			</join>
			<join src_field='id' target_field='label'>
				<table name='label_tag' />
			</join>
		</table>
	</group>
	
	<!-- Work  -->
	<group root_table='work'>
		<table name='work'>
			<join src_field='id' target_field='work'>
				<table name='work_alias' />
			</join>
			<join src_field='id' target_field='work'>
				<table name='work_tag' />
			</join>
			<join src_field='artist_credit' target_field='artist_credit'>
				<table name='artist_credit_name'>
					<join src_field='artist' target_field='id'>
						<table name='artist' />
					</join>
				</table>
			</join>
		</table>
	</group>
	
	<!-- ReleaseGroup  -->
	<group root_table='release_group'>
		<table name='release_group'>
			<join src_field='id' target_field='release_group'>
				<table name='release' />
			</join>
			<join src_field='id' target_field='release_group'>
				<table name='release_group_tag' />
			</join>
			<join src_field='artist_credit' target_field='artist_credit'>
				<table name='artist_credit_name'>
					<join src_field='artist' target_field='id'>
						<table name='artist' />
					</join>
				</table>
			</join>
		</table>
	</group>
	
	<!-- Release  -->
	<group root_table='release'>
		<table name='release'>
			<join src_field='id' target_field='release'>
				<table name='release_label'>
					<join src_field='label' target_field='id'>
						<table name='label' />
					</join>
				</table>
			</join>
			<join src_field='id' target_field='release'>
				<table name='medium'>
					<join src_field='tracklist' target_field='id'>
						<table name='tracklist' />
					</join>
					<join src_field='id' target_field='medium'>
						<table name='medium_cdtoc' />
					</join>
				</table>					
			</join>
			<join src_field='id' target_field='id'>
				<table name='release_meta' />
			</join>
			<join src_field='release_group' target_field='id'>
				<table name='release_group' />
			</join>
			<join src_field='id' target_field='release'>
				<table name='medium'>
					<join src_field='tracklist' target_field='id'>
						<table name='tracklist'>
							<join src_field='id' target_field='tracklist'>
								<table name='track'>
									<join src_field='recording' target_field='recording'>
										<table name ='recording_puid' />
									</join>
								</table>
							</join>
						</table>
					</join>
				</table>					
			</join>
			<join src_field='artist_credit' target_field='artist_credit'>
				<table name='artist_credit_name'>
					<join src_field='artist' target_field='id'>
						<table name='artist' />
					</join>
				</table>
			</join>
		</table>
	</group>
	
	<!-- Recording  -->
	<group root_table='recording'>
		<table name='recording'>
			<join src_field='id' target_field='recording'>
				<table name='recording_puid' />
			</join>
			<join src_field='id' target_field='recording'>
				<table name='recording_tag' />
			</join>
			<join src_field='id' target_field='recording'>
				<table name='isrc' />
			</join>
			<join src_field='id' target_field='recording'>
				<table name='track'>
					<join src_field='tracklist' target_field='id'>
						<table name='tracklist'>
							<join src_field='id' target_field='tracklist'>
								<table name='medium'>
									<join src_field='release' target_field='id'>
										<table name='release'>
											<join src_field='release_group' target_field='id'>
												<table name='release_group' />
											</join>
										</table>
									</join>
								</table>
							</join>
						</table>
					</join>
				</table>					
			</join>
			<join src_field='artist_credit' target_field='artist_credit'>
				<table name='artist_credit_name'>
					<join src_field='artist' target_field='id'>
						<table name='artist' />
					</join>
				</table>
			</join>
		</table>
	</group>
	
	<!-- Tag  -->
	<group root_table='tag'>
		<table name='tag' />
	</group>
	
	<!-- Annotation  -->
	<group root_table='annotation'>
		<table name='annotation'>
			<join src_field='id' target_field='annotation'>
				<table name='artist_annotation'>
					<join src_field='artist' target_field='id'>
						<table name='artist' />
					</join>
				</table>
			</join>
			<join src_field='id' target_field='annotation'>
				<table name='label_annotation'>
					<join src_field='label' target_field='id'>
						<table name='label' />
					</join>
				</table>
			</join>
			<join src_field='id' target_field='annotation'>
				<table name='work_annotation'>
					<join src_field='work' target_field='id'>
						<table name='work' />
					</join>
				</table>
			</join>
			<join src_field='id' target_field='annotation'>
				<table name='release_annotation'>
					<join src_field='release' target_field='id'>
						<table name='release' />
					</join>
				</table>
			</join>
			<join src_field='id' target_field='annotation'>
				<table name='release_group_annotation'>
					<join src_field='release_group' target_field='id'>
						<table name='release_group' />
					</join>
				</table>
			</join>
			<join src_field='id' target_field='annotation'>
				<table name='recording_annotation'>
					<join src_field='recording' target_field='id'>
						<table name='recording' />
					</join>
				</table>
			</join>
		</table>
	</group>
</table_dependencies>